<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.easyfit_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".dialogs.DeleteWorkoutDialogFragment"></activity>
        <activity android:name=".activities.AboutActivity" />
        <activity android:name=".activities.SavedWorkoutActivity" />
        <activity android:name=".activities.BrowseWorkoutsActivity" />
        <activity android:name=".activities.AllExercisesActivity" />
        <activity android:name=".activities.WorkoutActivity" />
        <activity android:name=".activities.EquipmentActivity" />
        <activity android:name=".activities.MuscleGroupActivity" />
        <activity android:name=".activities.GoalActivity" />
        <activity android:name=".activities.LevelActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>